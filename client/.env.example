# Kiro-mini Frontend Environment Variables

# Application Environment
REACT_APP_ENV=development

# API Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_WS_URL=ws://localhost:8000

# DICOM Services Configuration
REACT_APP_DICOM_WEB_URL=http://localhost:8080/dicomweb
REACT_APP_WADO_RS_URL=http://localhost:8080/dicomweb
REACT_APP_QIDO_RS_URL=http://localhost:8080/dicomweb
REACT_APP_STOW_RS_URL=http://localhost:8080/dicomweb

# Orthanc Configuration
REACT_APP_ORTHANC_URL=http://localhost:8042

# AI Services Configuration
REACT_APP_AI_SERVICE_URL=http://localhost:5000
REACT_APP_AI_ENHANCEMENT_ENABLED=true

# Collaboration Services
REACT_APP_COLLABORATION_URL=http://localhost:3002
REACT_APP_COLLABORATION_ENABLED=false

# Feature Flags
REACT_APP_ENABLE_AI_ASSIST=true
REACT_APP_ENABLE_REALTIME_BILLING=true
REACT_APP_ENABLE_WORKFLOW_TIMER=true
REACT_APP_ENABLE_3D_NAVIGATION=true
REACT_APP_ENABLE_ADVANCED_TOOLS=true
REACT_APP_ENABLE_STRUCTURED_REPORTING=true

# Security Configuration
REACT_APP_AUTH_ENABLED=false
REACT_APP_JWT_SECRET_KEY=your-jwt-secret-key-here

# Performance Configuration
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_CACHE_SIZE_MB=512
REACT_APP_MAX_CONCURRENT_REQUESTS=10

# Development Settings
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=info
REACT_APP_ENABLE_REMOTE_LOGGING=false
REACT_APP_LOG_ENDPOINT=http://localhost:3001/api/logs

# Network Configuration
REACT_APP_REQUEST_TIMEOUT=30000
REACT_APP_RETRY_ATTEMPTS=3
REACT_APP_ENABLE_OFFLINE_MODE=false

# Build Configuration
GENERATE_SOURCEMAP=true
SKIP_PREFLIGHT_CHECK=true