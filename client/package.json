  "scripts": {
    "start": "react-app-rewired start",
    "start:5004": "PORT=5004 react-app-rewired start",
    "build": "set SKIP_PREFLIGHT_CHECK=true && set TSC_COMPILE_ON_ERROR=true && react-app-rewired build",
    "test": "react-app-rewired test",
    "test:performance": "jest --config=jest.config.js --testMatch='**/*performance*.test.ts' --runInBand --detectOpenHandles",
    "test:load": "jest --config=jest.config.js --testMatch='**/*load*.test.ts' --runInBand --detectOpenHandles",
    "test:integration": "jest --config=jest.integration.config.js --runInBand",
    "test:all": "npm run test:performance && npm run test:load && npm run test:integration",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
